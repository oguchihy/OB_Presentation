#Confirm path to Dropbox
print(Sys.getenv("DROPBOX_PATH"))
#If successful,
# Get the Dropbox path from the environment variable
dropbox_path <- Sys.getenv("DROPBOX_PATH")

# Build a path to a specific project or file within your Dropbox
data_path <- file.path(dropbox_path, "CSVS", "OB Service", "Analysis", "datafile.csv")

# Use the path in functions, e.g., to read data
if (file.exists(data_path)) {
    data <- read.csv(data_path)
} else {
    stop("Data file does not exist:", data_path)
}

